package fundamentos;

import java.util.Scanner;

public class Wrapper {
	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		
		//byte
		Byte b = 100;
		Short s = 1000;
		//recebendo um valor digitado e convertendo para Integer
		Integer i = Integer.parseInt(entrada.next()); //tipo primitivo é o int
		Long l = 100000L; //tipo primitivo = long / classe = Long
		
		System.out.println(b.byteValue());
		System.out.println(s.toString());
		System.out.println(i*3);
		
		
		
		entrada.close();
	}
}
