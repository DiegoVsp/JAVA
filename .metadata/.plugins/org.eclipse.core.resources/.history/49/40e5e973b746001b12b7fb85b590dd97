package fundamentos;

public class ConversaotipoPrimitivoNumerico {

	public static void main(String[] args) {
		double a = 1; //implícita	
		System.out.println(a);
		
		//conversão explícita(CAST)
		float b =(float) 1.0;
		System.out.println(b);
		
		int c = 130;
		byte d = (byte)c;
		
		System.out.println(d);
	}
}
